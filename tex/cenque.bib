	Eprint = {1005.3022},
	Journal = {\mnras},
	Keywords = {galaxies: clusters: general, cosmology: theory, large-scale structure of Universe},
	Month = nov,
	Pages = {1818-1834},
	Title = {{Cluster galaxy dynamics and the effects of large-scale environment}},
