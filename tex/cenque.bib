	Eprint = {astro-ph/0601295},
	Journal = {\mnras},
	Keywords = {shock waves: cooling flows: galaxies: evolution: galaxies: formation: galaxies: haloes: galaxies: ISM, shock waves, cooling flows, galaxies: evolution, galaxies: formation, galaxies: haloes, galaxies: ISM},
	Month = aug,
