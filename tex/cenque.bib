	Journal = {\apj},
	Keywords = {cosmology: observations, galaxies: evolution, galaxies: halos},
	Month = dec,
	Pages = {93},
	Title = {{Evolution of the Stellar-to-dark Matter Relation: Separating Star-forming and Passive Galaxies from z = 1 to 0}},
	Volume = 778,
	Year = 2013,
	Bdsk-File-1 = {},
