	Journal = {\mnras},
	Keywords = {methods: numerical, cooling flows, galaxies: evolution, galaxies: formation},
	Month = oct,
	Pages = {2-28},
	Title = {{How do galaxies get their gas?}},
	Volume = 363,
	Year = 2005,
