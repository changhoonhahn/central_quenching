	Doi = {10.1086/505628},
	Eprint = {astro-ph/0512127},
	Journal = {\apj},
	Keywords = {Galaxies: Evolution},
	Month = sep,
	Pages = {268-280},
	Title = {{Galaxies in SDSS and DEEP2: A Quiet Life on the Blue Sequence?}},
	Volume = 648,
	Year = 2006,
