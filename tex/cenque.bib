	Doi = {10.1046/j.1365-8711.2003.06955.x},
	Eprint = {astro-ph/0302161},
	Journal = {\mnras},
	Keywords = {hydrodynamics, shock waves, cooling flows, galaxies: formation, galaxies: ISM, dark matter},
	Month = oct,
	Pages = {349-364},
	Title = {{Virial shocks in galactic haloes?}},
