	Doi = {10.1111/j.1365-2966.2007.12074.x},
	Eprint = {astro-ph/0703435},
	Journal = {\mnras},
	Keywords = {accretion, accretion discs, shock waves, galaxies: evolution, galaxies: formation, galaxies: haloes, dark matter},
