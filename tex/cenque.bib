	Keywords = {galaxies: dwarf, galaxies: stellar content, methods: statistical},
	Month = sep,
	Pages = {85},
	Title = {{A Stellar Mass Threshold for Quenching of Field Galaxies}},
	Volume = 757,
	Year = 2012,
	Bdsk-File-1 = {},
	Bdsk-Url-1 = {http://dx.doi.org/10.1088/0004-637X/757/1/85},
