	Doi = {10.1093/mnras/stv318},
	Eprint = {1409.1574},
	Journal = {\mnras},
	Keywords = {ISM: atoms, ISM: molecules, galaxies: evolution, galaxies: formation, galaxies: ISM},
	Month = may,
	Pages = {477-493},
	Title = {{Evolution of the atomic and molecular gas content of galaxies in dark matter haloes}},
