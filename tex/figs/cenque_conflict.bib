	Author = {{Wetzel}, A.~R. and {Tinker}, J.~L. and {Conroy}, C. and {van den Bosch}, F.~C.},
	Date-Added = {2016-04-19 17:39:49 +0000},
	Date-Modified = {2016-04-19 17:39:49 +0000},
	Doi = {10.1093/mnras/stt469},
	Eprint = {1206.3571},
	Journal = {\mnras},
	Keywords = {methods: numerical, galaxies: clusters: general, galaxies: evolution, galaxies: groups: general, galaxies: haloes, galaxies: star formation},
	Month = jun,
	Pages = {336-358},
	Title = {{Galaxy evolution in groups and clusters: satellite star formation histories and quenching time-scales in a hierarchical Universe}},
