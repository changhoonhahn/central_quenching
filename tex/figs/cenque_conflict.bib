	Arxivurl = {http://arXiv.org/abs/astro-ph/0607512},
	Author = {{Cooper}, M.~C. and {Newman}, J.~A. and {Coil}, A.~L. and {Croton}, D.~J. and {Gerke}, B.~F. and {Yan}, R. and {Davis}, M. and {Faber}, S.~M. and {Guhathakurta}, P. and {Koo}, D.~C. and {Weiner}, B.~J. and {Willmer}, C.~N.~A.},
	Date-Added = {2016-04-26 19:23:12 +0000},
