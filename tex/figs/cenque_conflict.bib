	Eprint = {astro-ph/0512234},
	Journal = {\apj},
	Keywords = {Cosmology: Theory, Cosmology: Dark Matter, Galaxies: Clusters: General, Galaxies: Evolution, Galaxies: Halos, Cosmology: Large-Scale Structure of Universe},
	Month = aug,
	Pages = {201-214},
