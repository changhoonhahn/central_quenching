	Journal = {\apj},
	Keywords = {cosmology: theory, dark matter, galaxies: abundances, galaxies: evolution, galaxies: statistics},
	Month = mar,
	Pages = {33},
	Title = {{Tracing Galaxies through Cosmic Time with Number Density Selection}},
